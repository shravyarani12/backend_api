    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Dev Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV":"dev"
            },
            "program": "${workspaceFolder}/server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Prod Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV":"prod"
            },
            "program": "${workspaceFolder}/server.js"
        }
    ]
}